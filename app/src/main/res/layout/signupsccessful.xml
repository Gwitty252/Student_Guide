<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">


    <ImageView
        android:id="@+id/correct"
        android:layout_width="320dp"
        android:layout_height="324dp"
        android:layout_marginTop="40dp"
        android:src="@drawable/correct1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.648"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/registration_succe"
        android:layout_width="347dp"
        android:layout_height="55dp"
        android:layout_marginBottom="16dp"
        android:gravity="center_horizontal|center_vertical"
        android:lineSpacingExtra="-4sp"
        android:text="Registration Successful "
        android:textStyle="bold"
        android:textAppearance="@style/login_succe"
        app:layout_constraintBottom_toTopOf="@+id/hang_out_yo"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/hang_out_yo"
        android:layout_width="289dp"
        android:layout_height="38dp"
        android:layout_marginBottom="312dp"
        android:gravity="center_horizontal|center_vertical"
        android:lineSpacingExtra="1sp"
        android:text="@string/hang_out_yo"
        android:textAppearance="@style/hang_out_yo"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />



</androidx.constraintlayout.widget.ConstraintLayout>